                  <Font size="10.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="group" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="outsourced" layoutX="187.0" layoutY="32.0" mnemonicParsing="false" onAction="#outsourced" selected="true" text="Outsourced" toggleGroup="$group">
